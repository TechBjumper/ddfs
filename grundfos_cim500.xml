        <stateSensor ruleid="pumpStatus_bit1">
            <type>state</type>
            <sensorId>PUMP_STATUS_BIT1</sensorId>
            <value>
                <mapValue mapid="pumpStatus-vm">
                    <!-- Bit 1 -->
                    <formatNumber printffmt="%1.0f"> 
                        <bitwise_and>
                        <op>
                        <right_shift>
                        <op><getOid>PUMP_STATUS</getOid></op>
                        <op>1</op>
                        </right_shift>
                        </op>
                        <op>1</op>
                        </bitwise_and>
                    </formatNumber>
                </mapValue>
                </value>
                <label>Copy to Local (Settings/Op Mode/Control Mode/On-Off State)</label>
                <enum>pumpStatus_bit1-em</enum>
                <sensorSet>Status</sensorSet>
        </stateSensor>
		<stateSensor ruleid="pumpStatus_bit2">
            <type>state</type>
            <sensorId>PUMP_STATUS_BIT2</sensorId>
            <value>
                <mapValue mapid="pumpStatus-vm">
                    <!-- Bit 2 -->
                    <formatNumber printffmt="%1.0f"> 
                        <bitwise_and>
                        <op>
                        <right_shift>
                        <op><getOid>PUMP_STATUS</getOid></op>
                        <op>2</op>
                        </right_shift>
                        </op>
                        <op>1</op>
                        </bitwise_and>
                    </formatNumber>
                </mapValue>
                </value>
                <label>Max Flow Limit Enabled (MAGNA3/MGE Model H+)</label>
                <enum>pumpStatus_bit2-em</enum>
                <sensorSet>Status</sensorSet>
        </stateSensor>
		<stateSensor ruleid="pumpStatus_bit3">
            <type>state</type>
            <sensorId>PUMP_STATUS_BIT3</sensorId>
