                    <formatNumber printffmt="%1.0f"> 
                        <bitwise_and>
                        <op>
                        <right_shift>
                        <op><getOid>PUMP_STATUS</getOid></op>
                        <op>3</op>
                        </right_shift>
                        </op>
                        <op>1</op>
                        </bitwise_and>
                    </formatNumber>
                </mapValue>
                </value>
                <label>Reset Alarm Ack</label>
                <enum>pumpStatus_bit3-em</enum>
                <sensorSet>Status</sensorSet>
        </stateSensor>
		<stateSensor ruleid="pumpStatus_bit4">
            <type>state</type>
            <sensorId>PUMP_STATUS_BIT4</sensorId>
            <value>
                <mapValue mapid="pumpStatus-vm">
