                    <formatNumber printffmt="%1.0f"> 
                        <bitwise_and>
                        <op>
                        <right_shift>
                        <op><getOid>PUMP_STATUS</getOid></op>
                        <op>4</op>
                        </right_shift>
                        </op>
                        <op>1</op>
                        </bitwise_and>
                    </formatNumber>
                </mapValue>
                </value>
