			<value>
				<getOid>Function_3_Reg_964</getOid>
			</value>
			<alarmFlagsTest mask="1" match="1">
				<name>
					<activeText>Input Contact 1 Status Alarm present</activeText>
					<inactiveText>Input Contact 1 Status Alarm Cleared</inactiveText>
				</name>
				<severity>warning</severity>
				<category>System</category>
				<description>
					<activeText>A Input Contact 1 Status Alarm condition exists.</activeText>
					<inactiveText>A Input Contact 1 Status Alarm condition no longer exists.</inactiveText>
				</description>
			</alarmFlagsTest>
<alarmFlagsTest mask="2" match="2">
				<name>
					<activeText>Input Contact 2 Status Alarm present</activeText>
