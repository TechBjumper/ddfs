--
-- Traps
--

--Map of types used in traps to those used in requests
--	Monnit-EGW4-Trap-DevIDReported ::= Monnit-EGW4-GatewayID
--	Monnit-EGW4-Trap-AllValuesReported ::=
-- 		CHOICE {
--							Monnit-EGW4-trap-values-translatable Monnit-EGW4-SensorDeviceReadingValuesAll,
--							Monnit-EGW4-trap-values-unknown Monnit-EGW4-SensorDeviceReadingUntranslated
--					 }
--	Monnit-EGW4-Trap-AllUnitsReported ::= Monnit-EGW4-SensorDeviceReadingUnitsAll
--	Monnit-EGW4-Trap-AgeReported ::= Monnit-EGW4-SensorDeviceReadingUnitsAll
--	Monnit-EGW4-Trap-AlarmingReported ::= Monnit-EGW4-SensorDeviceReadingUnitsAll

MonnitEGW4TrapDevIDReported ::= TEXTUAL-CONVENTION
		STATUS        mandatory
		DESCRIPTION
				"The serial number identifier of the sensor device assigned and
				programmed by the manufacturer. This will ordinarily be printed
				on a label on the housing of the device. MonnitEGW4TrapDevIDReported
				is the same as MonnitEGW4SensorID."
		SYNTAX				Unsigned32(1..2147483631)

MonnitEGW4TrapAllValuesReported ::= TEXTUAL-CONVENTION
		STATUS        mandatory
		DESCRIPTION
				"This will make one of two reports. For the case the sensor type is
				known to the gateway, this will be all of the datums for the most
 				recent reading of the sensor device, separated by commas, in textual
 				format, using the MonnitEGW4SensorDeviceReadingValuesAll format.
				Where the sensor type is not known to the gateway, this will be
				the untranslated data block received from a sensor, in hexadecimal
				format. This MonnitEGW4TrapAllValuesReported is the same as either
				MonnitEGW4SensorDeviceReadingValuesAll or
 				MonnitEGW4SensorDeviceReadingUntranslated."
		SYNTAX				OCTET STRING

MonnitEGW4TrapAllUnitsReported ::= TEXTUAL-CONVENTION
		STATUS        mandatory
		DESCRIPTION
				"All of the units corresponding to the datums for the most recent
				reading of the sensor device, separated by commas, in textual
				format. For example, for a sensor reporting temperature and
				humidity this might read 'degrees C, %RH'.
				MonnitEGW4TrapAllUnitsReported is the same as
 				MonnitEGW4SensorDeviceReadingUnitsAll."
		SYNTAX				OCTET STRING

MonnitEGW4TrapAgeReported ::= TEXTUAL-CONVENTION
		STATUS        mandatory
		DESCRIPTION
				"The age of the sensor report received most recently from a sensor.
				That time is reported as the number of seconds between the time of
				the sensor report and the time this information is requested.
				MonnitEGW4TrapAgeReported is the same as
				MonnitEGW4SensorDeviceReadingAge."
		SYNTAX				OCTET STRING

MonnitEGW4TrapAlarmingReported ::= TEXTUAL-CONVENTION
		STATUS        mandatory
		DESCRIPTION
				"Either 'Alarming' or 'Not alarming'. A device will report an alarm
				if it is programmed with a threshold found to be exceeded. Examples:
				a temperature sensor for a refrigeration unit can be programmed to
				alarm if the measured temperature rises above a threshold, and
				a door sensor might alarm if a security door is opened.
				MonnitEGW4TrapAlarmingReported is the same as
				MonnitEGW4SensorDeviceReadingAlarming."
		SYNTAX				OCTET STRING

monnitEGW4NewSensorTraffic TRAP-TYPE
	ENTERPRISE monnitEGW4
	VARIABLES {
							monnitEGW4SensorInfoTranslatedFormatSensorDeviceID, MonnitEGW4TrapDevIDReported,
							monnitEGW4SensorInfoTranslatedFormatSensorDeviceReadingValuesAll, MonnitEGW4TrapAllValuesReported,
							monnitEGW4SensorInfoTranslatedFormatSensorDeviceReadingUnitsAll, MonnitEGW4TrapAllUnitsReported,
							monnitEGW4SensorInfoTranslatedFormatSensorDeviceReadingAge, MonnitEGW4TrapAgeReported,
							monnitEGW4SensorInfoTranslatedFormatSensorDeviceAlarming, MonnitEGW4TrapAlarmingReported
						}
	DESCRIPTION
			"This trap, when enabled, will generate a report corresponding to new
 			data from any registered sensor as it is received. Included with the
 			trap message are the sensor ID, all values the sensor reports, units for
 			all those values, the age, and whether the sensor is alarming."
	::= 1

monnitEGW4DeviceAlarming TRAP-TYPE
	ENTERPRISE monnitEGW4
	VARIABLES {
							monnitEGW4SensorInfoTranslatedFormatSensorDeviceID, MonnitEGW4TrapDevIDReported,
							monnitEGW4SensorInfoTranslatedFormatSensorDeviceReadingValuesAll, MonnitEGW4TrapAllValuesReported,
							monnitEGW4SensorInfoTranslatedFormatSensorDeviceReadingUnitsAll, MonnitEGW4TrapAllUnitsReported,
							monnitEGW4SensorInfoTranslatedFormatSensorDeviceReadingAge, MonnitEGW4TrapAgeReported,
							monnitEGW4SensorInfoTranslatedFormatSensorDeviceAlarming, MonnitEGW4TrapAlarmingReported
						}
	DESCRIPTION
			"This trap, when enabled, will generate a report corresponding to new
 			data from any registered sensor as it is received, but only if that
			sensor is alarming and the monnitEGW4NewSensorTraffic trap is disabled.
			(That trap will generate the same message, rendering this trap redundant.)
			Included with the trap message are the sensor ID, all values the sensor
 			reports, units for all those values, the age, and whether the sensor
 			is alarming."
	::= 2

END
