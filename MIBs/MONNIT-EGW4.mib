 --
 -- Sensor-specific section, untranslated legacy format, by registration order
 --

monnitEGW4SensorInfoBySensorRegisOrderTable OBJECT-TYPE
		SYNTAX				SEQUENCE OF monnitEGW4SensorInfoBySensorRegisOrderSensorEntry
		ACCESS    		not-accessible
		STATUS        mandatory
		DESCRIPTION
				"The (conceptual) table containing an untranslated numeric format,
				addressed by sensor registration order starting at 1 and ascending by
				one, in a legacy integer format. This is provided for users who need a
 				precise match of the format and order of the older EGW1 platform.
 				The gateway will report the most current information received from
 				sensors, according to their programmed reporting period or heartbeat.
				A more convenient format to new users is available under
				the monnitEGW4SensorInfoTranslatedFormat. If queried, this
				OID will return the number of rows in the table, which is the number
				of sensors registered to this gateway."
		::= { monnitEGW4 4 }

monnitEGW4SensorInfoBySensorRegisOrderEntry OBJECT-TYPE
		SYNTAX				MonnitEGW4SensorInfoBySensorRegisOrderEntry
		ACCESS    		not-accessible
		STATUS        mandatory
		DESCRIPTION
				"An entry (conceptual row) in the untranslated by-registration-order
 				table containing the latest sensor state for a single sensor."
		INDEX   { monnitEGW4SensorInfoBySensorRegisOrderSensDevID }
		::= { monnitEGW4SensorInfoBySensorRegisOrderTable 1 }

MonnitEGW4SensorInfoBySensorRegisOrderEntry ::= SEQUENCE {
		monnitEGW4SensorInfoBySensorRegisOrderSensorDeviceID Unsigned32,
		monnitEGW4SensorInfoBySensorRegisOrderSensorDeviceWDIndexNo INTEGER,
		monnitEGW4SensorInfoBySensorRegisOrderSensorDeviceTypeNo INTEGER,
		monnitEGW4SensorInfoBySensorRegisOrderSensorDeviceReadingAge Unsigned32,
		monnitEGW4SensorInfoBySensorRegisOrderSensorDeviceActive INTEGER,
		monnitEGW4SensorInfoBySensorRegisOrderSensorDeviceAlarming INTEGER,
		monnitEGW4SensorInfoBySensorRegisOrderSensorDeviceVoltage INTEGER,
		monnitEGW4SensorInfoBySensorRegisOrderSensorDeviceRFStrength INTEGER,
		monnitEGW4SensorInfoBySensorRegisOrderSensorDeviceRptUT DisplayString,
