allow-Front-Panel-LVD-Control OBJECT-TYPE
  SYNTAX Boolean
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "If true LVD manual control from the front panel is allowed."
  DEFVAL { false }
::= { lvd 15 }

lvd1 OBJECT IDENTIFIER ::= { lvd 20 }

enable-LVD1 OBJECT-TYPE
  SYNTAX Enable-State
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "Enables operation of LVD1. If two LVDs are used, LVD1 may be used to connect non-essential load that may be disconnected during an AC failure to extend battery hold-up time and protect the essential load."
  DEFVAL { disabled }
::= { lvd1 5 }

lvd1-State OBJECT-TYPE
  SYNTAX LVD-State
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION "Indicates the state of LVD1: Connected or Disconnected."
::= { lvd1 10 }

lvd1-Inhibited OBJECT-TYPE
  SYNTAX Boolean
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION "While Inhibited, LVD1 cannot connect or reconnect. Indicates if the configurable Inhibit Period for LVD1 has elapsed since LVD1 last changed state. While true, LVD1 cannot change state again."
::= { lvd1 20 }

lvd1-AI-Disconnect-Trigger OBJECT-TYPE
  SYNTAX LVD-AI-Disconnect-Trigger
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "Specifies whether LVD1 will disconnect when a particular analogue input exceeds a threshold."
  DEFVAL { disabled }
::= { lvd1 30 }

lvd1-AI-Number OBJECT-TYPE
  SYNTAX Unsigned8
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "LVD 1 will disconnect if this analogue input exceeds the threshold(s) specified in LVD 1 Analogue Input Disconnect Trigger."
  DEFVAL { 1 }
::= { lvd1 40 }

lvd1-AC-Timer-Disconnect-Delay OBJECT-TYPE
  SYNTAX TimeIntervalInMinutes
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "If Enable AC Timer is set then LVD1 will disconnect after the Disconnect Delay from the start of an AC failure, unless Disconnect Voltage is reached first. Units: 'min'."
  DEFVAL { 240 }
::= { lvd1 50 }

enable-LVD1-AC-Timer OBJECT-TYPE
  SYNTAX Enable-State
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "LVD1 will disconnect after the AC Timer Disconnect Delay, even if the Disconnect Voltage is not reached."
  DEFVAL { disabled }
::= { lvd1 55 }

lvd1-Disconnect-Voltage OBJECT-TYPE
  SYNTAX WritableFloat
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "If the bus voltage drops to this value, then any shutdown rectifiers are restarted (even if LVD is disabled). Then, if the bus voltage is still lower than this value after the LVD1 Recognition Period, the LVD1 will disconnect. Units: '0.01 V'."
  DEFVAL { 4320 }
::= { lvd1 60 }

lvd1-Reconnect-Voltage OBJECT-TYPE
  SYNTAX WritableFloat
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "LVD1 reconnects when the bus voltage has been above this value for the LVD1 Recognition Period and LVD2 is disabled, or has already reconnected, or is in slave mode. Units: '0.01 V'."
  DEFVAL { 4800 }
::= { lvd1 70 }

lvd1-Recognition-Period OBJECT-TYPE
  SYNTAX TimeIntervalInSeconds
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "The time the bus voltage must be below the LVD1 Disconnect Voltage before disconnection, or above the LVD1 Reconnect Voltage before reconnection. Units: 's'."
  DEFVAL { 10 }
::= { lvd1 80 }

lvd2 OBJECT IDENTIFIER ::= { lvd 30 }

enable-LVD2 OBJECT-TYPE
  SYNTAX Enable-State
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "Enables operation of LVD2 (if used). If two LVDs are used LVD2 can be used to disconnect the essential load, or the batteries, that should be only disconnected after LVD1 has disconnected."
  DEFVAL { disabled }
::= { lvd2 5 }

enable-LVD2-Slave-Mode OBJECT-TYPE
  SYNTAX Enable-State
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "LVD2 will disconnect and reconnect at the same voltages as LVD1. The LVD2 configuration parameters are ignored."
  DEFVAL { disabled }
::= { lvd2 7 }

lvd2-State OBJECT-TYPE
  SYNTAX LVD-State
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION "Indicates the state of the LVD2: Connected or Disconnected."
::= { lvd2 10 }

lvd2-Inhibited OBJECT-TYPE
  SYNTAX Boolean
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION "While Inhibited, LVD2 cannot connect or reconnect. Indicates if the configurable Inhibit Period for LVD2 has elapsed since LVD1 last changed state. While true, LVD2 cannot change state again."
::= { lvd2 20 }

lvd2-AI-Disconnect-Trigger OBJECT-TYPE
  SYNTAX LVD-AI-Disconnect-Trigger
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "Specifies whether LVD2 will disconnect when a particular analogue input exceeds a threshold."
  DEFVAL { disabled }
::= { lvd2 30 }

lvd2-AI-Number OBJECT-TYPE
  SYNTAX Unsigned8
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "LVD 2 will disconnect if this analogue input exceeds the threshold(s) specified in LVD 2 Analogue Input Disconnect Trigger."
  DEFVAL { 1 }
::= { lvd2 40 }

lvd2-AC-Timer-Disconnect-Delay OBJECT-TYPE
  SYNTAX TimeIntervalInMinutes
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "If Enable AC Timer is set then the LVD2 will disconnect after the LVD2 AC Timer Disconnect Delay from the start of an AC failure, unless LVD2 Disconnect Voltage is reached first. Units: 'min'."
  DEFVAL { 240 }
::= { lvd2 50 }

enable-LVD2-AC-Timer OBJECT-TYPE
  SYNTAX Enable-State
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "LVDs will disconnect after the AC Timer Disconnect Delay, even if the Disconnect Voltage is not reached."
  DEFVAL { disabled }
::= { lvd2 55 }

lvd2-Disconnect-Voltage OBJECT-TYPE
  SYNTAX WritableFloat
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "If the bus voltage drops to this value after the LVD2 Recognition Period, the LVD2 will disconnect. This parameter is ignored if Enable LVD2 Slave Mode is Enabled. Units: '0.01 V'."
  DEFVAL { 4320 }
::= { lvd2 60 }

lvd2-Reconnect-Voltage OBJECT-TYPE
  SYNTAX WritableFloat
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "LVD2 reconnects when the bus voltage has been above this value for the LVD2 Recognition Period. This parameter is ignored if Enable LVD2 Slave Mode is Enabled. Units: '0.01 V'."
  DEFVAL { 4800 }
::= { lvd2 70 }

lvd2-Recognition-Period OBJECT-TYPE
  SYNTAX TimeIntervalInSeconds
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "The time the bus voltage must be below the LVD2 Disconnect Voltage before disconnection, or above the LVD2 Reconnect Voltage before reconnection. Units: 's'."
  DEFVAL { 10 }
::= { lvd2 80 }

battery-Test OBJECT IDENTIFIER ::= { control-Functions 70 }

enable-Battery-Test OBJECT-TYPE
  SYNTAX Enable-State
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "Periodically tests the condition of the battery by reducing the rectifier voltage and allowing the batteries to discharge into the normal load."
  DEFVAL { disabled }
::= { battery-Test 3 }

battery-Test-Start-Allowed OBJECT-TYPE
  SYNTAX Can-Start
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION "Indicates if a Battery Test is allowed to start. Battery Tests cannot start within 48 hours of an AC failure, if there are no rectifiers, or if Equalize or Fast Charge is active."
::= { battery-Test 7 }

battery-Test-Lockout-Remaining OBJECT-TYPE
  SYNTAX TimeIntervalInSeconds
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION "How much longer the Battery Test is locked-out. Battery Tests cannot be started within 48 hours of an AC failure. If an immediate test is required, set Battery test Interval to zero (this disables the 48 hour lockout). The value is NaN if BT is active or disabled. Units: 's'."
::= { battery-Test 8 }

battery-Test-State OBJECT-TYPE
  SYNTAX Battery-Test-State
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION "The state of the Battery Test process."
::= { battery-Test 10 }

battery-Test-First-Date-Time OBJECT-TYPE
  SYNTAX Julian-Time-UTC
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION "The date and time that the first Battery Test will occur. Subsequent tests will occur at every Battery Test Interval after that. Value is stored as number of seconds since 1 January 1970."
::= { battery-Test 15 }

battery-Test-Interval OBJECT-TYPE
  SYNTAX TimeIntervalInDays
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "The time between scheduled Battery Tests. The interval period begins at the start of a Battery Test. Zero disables scheduled Battery Tests. Zero also disables the 48 hour lockout following an AC failure, allowing an immediate manual test. Units: 'd'."
  DEFVAL { 183 }
::= { battery-Test 17 }

next-Battery-Test-Start-Time OBJECT-TYPE
  SYNTAX Julian-Time-UTC
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION "The start time of the next scheduled Battery Test. The value is NaN if Battery Test is active or Periodic Battery Test function is disabled."
::= { battery-Test 20 }

battery-Test-Duration OBJECT-TYPE
  SYNTAX TimeIntervalInMinutes
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "The maximum time a Battery Test process will be active. The Battery Test will pass if the bus voltage remains above the Battery Test Termination Voltage for the duration of the test. Units: 'min'."
  DEFVAL { 30 }
::= { battery-Test 30 }

battery-Test-Remaining-Time OBJECT-TYPE
  SYNTAX TimeIntervalInSeconds
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION "The time to the end of the currently active Battery Test. The value is NaN if Battery Test is inactive. Units: 's'."
::= { battery-Test 40 }

battery-Test-Termination-Voltage OBJECT-TYPE
  SYNTAX WritableFloat
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "If the bus voltages drops below this value during a Battery Test, then the test fails. Units: '0.01 V'."
  DEFVAL { 4750 }
::= { battery-Test 50 }

battery-Test-Offset-Voltage OBJECT-TYPE
  SYNTAX Float
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION "The adjustment to the bus voltage being applied due to the Battery Test. While a Battery Test is running, the rectifiers are turned down to force the battery to carry the load. Units: '0.01 V'."
::= { battery-Test 60 }

start-Battery-Test OBJECT-TYPE
  SYNTAX No-Data
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "This command attempts to start the Battery Test process."
::= { battery-Test 100 }

stop-Battery-Test OBJECT-TYPE
  SYNTAX No-Data
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "This command stops the Battery Test process if it is currently active."
::= { battery-Test 110 }

