fast-Charge OBJECT IDENTIFIER ::= { control-Functions 40 }

enable-Fast-Charge OBJECT-TYPE
  SYNTAX Enable-State
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "After an AC power outage, Fast Charge automatically increases the float voltage of the DC power system to recharge the batteries as quickly as possible. Enable Fast Charge if the site experiences frequent AC power outages."
  DEFVAL { disabled }
::= { fast-Charge 5 }

fast-Charge-State OBJECT-TYPE
  SYNTAX Control-Process-State
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION "Indicates the state of Fast Charge process: Disabled, Inactive, Active, Pending or Suspended."
::= { fast-Charge 10 }

fast-Charge-Start-Allowed OBJECT-TYPE
  SYNTAX Can-Start
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION "Indicates if an Equalize cycle Fast Charge is allowed to start or the reason why not. Fast Charge cannot start if there are no rectifiers with AC power, or the battery is not discharged."
::= { fast-Charge 12 }

fast-Charge-Voltage OBJECT-TYPE
  SYNTAX WritableFloat
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "The bus voltage maintained during Fast Charge (use the value recommended by the battery manufacturer). Adjustments for Temperature Compensation and Battery Current Limit also apply. Units: '0.01 V'."
  DEFVAL { 5600 }
::= { fast-Charge 15 }

fast-Charge-Offset-Voltage OBJECT-TYPE
  SYNTAX Float
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION "The adjustment to the bus voltage being applied due to the Fast Charge. Units: '0.01 V'."
::= { fast-Charge 17 }

fast-Charge-Ah-Discharged OBJECT-TYPE
  SYNTAX Float
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION "The current level of battery discharge. Units: 'Ah'."
::= { fast-Charge 20 }

fast-Charge-Maximum-Duration OBJECT-TYPE
  SYNTAX TimeIntervalInMinutes
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "The maximum duration of a Fast Charge (use the value recommended by the battery manufacturer). Units: 'min'."
  DEFVAL { 1440 }
::= { fast-Charge 30 }

fast-Charge-Ampere-Hour-Threshold OBJECT-TYPE
  SYNTAX Unsigned8
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "If Fast Charge Ah Discharged is above this value, then Fast Charge occurs when AC power is restored. The threshold is given as a percentage of installed C10 battery capacity. Fast charge can also be triggered based on the Fast Charge Voltage Threshold. Units: '%'."
  DEFVAL { 25 }
::= { fast-Charge 35 }

fast-Charge-Voltage-Threshold OBJECT-TYPE
  SYNTAX WritableFloat
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "If the bus voltage drops below this value then Fast Charge occurs when AC power is restored. Fast charge can also be triggered based on the Fast Charge Ampere Hour Threshold. Units: '0.01 V'."
  DEFVAL { 4800 }
::= { fast-Charge 40 }

fast-Charge-Recharge-Percentage OBJECT-TYPE
  SYNTAX Unsigned8
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "The ratio of recharge ampere-hours required to restore the battery to fully charged status to the ampere-hours discharged. Units: '%'."
  DEFVAL { 110 }
::= { fast-Charge 45 }

fast-Charge-Maximum-Time-Remaining OBJECT-TYPE
  SYNTAX TimeIntervalInSeconds
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION "This timer is started when a Fast Charge cycle starts and runs while Fast Charge remains active. It is suspended and resumed when Fast Charge is suspended and resumed respectively. The Fast Charge cycle will stop if this timer reaches zero. The value is 0 if Fast Charge is inactive. Units: 's'."
::= { fast-Charge 50 }

stop-Fast-Charge OBJECT-TYPE
  SYNTAX No-Data
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "This command stops the Fast Charge process if it is currently active."
::= { fast-Charge 100 }

battery-Current-Limit OBJECT IDENTIFIER ::= { control-Functions 50 }

enable-Battery-Current-Limit OBJECT-TYPE
  SYNTAX Enable-State
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "Protects the batteries from excessive charge current. BCL varies the system voltage in order to maintain battery charge current below the Battery Current Limit value."
  DEFVAL { disabled }
::= { battery-Current-Limit 5 }

bcl-State OBJECT-TYPE
  SYNTAX BCL-State
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION "The state of the Battery Current Limit process."
::= { battery-Current-Limit 10 }

engine-Run-State OBJECT-TYPE
  SYNTAX Engine-Run-State
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION "The state of the Engine Run input. Engine Run is used to indicate whether AC power is being supplied by a generator or similar equipment, and a lower battery current limit is required."
::= { battery-Current-Limit 20 }

bcl-Limit OBJECT-TYPE
  SYNTAX Unsigned8
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "BCL maintains the battery current below this value, which is a percentage of the installed C10 Battery Capacity. Units: '%'."
  DEFVAL { 10 }
::= { battery-Current-Limit 30 }

bcl-Offset-Voltage OBJECT-TYPE
  SYNTAX Float
  ACCESS read-only
  STATUS mandatory
  DESCRIPTION "The bus voltage adjustment made by Battery Current Limit is applied to the Operating Voltage to produce the Target Voltage. Target Voltage is used as the input to the AVC function. Units: '0.01 V'."
::= { battery-Current-Limit 40 }

bcl-Engine-Run-Limit OBJECT-TYPE
  SYNTAX Unsigned8
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "The Battery Current Limit setting when Engine Run is active. BCL maintains the battery current below this value when the Engine Run digital input is active (Engine Run is enabled). This limit is expressed as a percentage of the installed C10 Battery Capacity. Units: '%'."
  DEFVAL { 2 }
::= { battery-Current-Limit 50 }

lvd OBJECT IDENTIFIER ::= { control-Functions 60 }

lvd-Inhibit-Period OBJECT-TYPE
  SYNTAX TimeIntervalInSeconds
  ACCESS read-write
  STATUS mandatory
  DESCRIPTION "The minimum time an LVD stays connected or disconnected before it can change state. Units: 's'."
  DEFVAL { 10 }
