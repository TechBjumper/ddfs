            {
                p1GlobalLatchingAlarmStatus
            }
            STATUS              current
            DESCRIPTION         "Global Latching Alarm Status Bit Map: 0 HighHighCurrent, 1 HighCurrent, 2 LowCurrent, 3 LowLowCurrent, 4 AlarmOff, 8 HighVoltage, 9 LowVoltage"
            ::= { p1Traps 1 }
    
        p1GlobalNonLatchingAlarmStatusTrap NOTIFICATION-TYPE    -- 
            OBJECTS
            {
                p1GlobalLatchingAlarmStatus
            }
            STATUS              current
            DESCRIPTION         "Global Non-Latching Alarm Status Bit Map: 0 HighCurrent, 1 LowCurrent, 8 HighVoltage, 9 LowVoltage"
            ::= { p1Traps 2 }
    
        p1VoltagePhaseAlarmStatusTrap NOTIFICATION-TYPE    -- voltage alarms A, B, C. Return phase index & status register.
            OBJECTS
            {
                p1AuxAlarmPhase,
                p1VoltagePhaseAlarmStatus 
            }
            STATUS              current
            DESCRIPTION         "Voltage Alarm Status Bit Map: 0 HighLatching, 1 LowLatching, 8 HighNonLatching, 9 LowNonLatching"
            ::= { p1Traps 3 }
    
