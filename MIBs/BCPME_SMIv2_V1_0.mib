    -- Alarm Traps 

    p2Traps OBJECT IDENTIFIER ::= { p2Alarms 0 } 
          
        p2GlobalLatchingAlarmStatusTrap NOTIFICATION-TYPE    -- 
            OBJECTS
            {
                p2GlobalLatchingAlarmStatus
            }
            STATUS              current
            DESCRIPTION         "Global Latching Alarm Status Bit Map: 0 HighHighCurrent, 1 HighCurrent, 2 LowCurrent, 3 LowLowCurrent, 4 AlarmOff, 8 HighVoltage, 9 LowVoltage"
            ::= { p2Traps 1 }
    
        p2GlobalNonLatchingAlarmStatusTrap NOTIFICATION-TYPE    -- 
            OBJECTS
            {
                p2GlobalLatchingAlarmStatus
            }
            STATUS              current
            DESCRIPTION         "Global Non-Latching Alarm Status Bit Map: 0 HighCurrent, 1 LowCurrent, 8 HighVoltage, 9 LowVoltage"
            ::= { p2Traps 2 }
    
        p2VoltagePhaseAlarmStatusTrap NOTIFICATION-TYPE    -- voltage alarms A, B, C. Return phase index & status register.
            OBJECTS
            {
                p2AuxAlarmPhase,
                p2VoltagePhaseAlarmStatus 
            }
            STATUS              current
            DESCRIPTION         "Voltage Alarm Status Bit Map: 0 HighLatching, 1 LowLatching, 8 HighNonLatching, 9 LowNonLatching"
            ::= { p2Traps 3 }
    
        p2AuxCurrentAlarmStatusTrap NOTIFICATION-TYPE    -- current alarms A, B, C, N. Return phase index & status register.
            OBJECTS
            {
                p2AuxAlarmPhase,
                p2AuxCurrentAlarmStatus  
            }
            STATUS              current
            DESCRIPTION         "Auxiliary Current Alarm Status Bit Map: 0 HighHighLatching, 1 HighLatching, 2 LowLatching, 3 LowLowLatching, 4 AlarmOff, 8 HighNonLatching, 9 LowNonLatching"
            ::= { p2Traps 4 }
    
        p2BranchCurrentAlarmStatusTrap NOTIFICATION-TYPE    -- branch current alarms 1-42. Return branch index & status register.
            OBJECTS
            {
                p2BranchAlarmNumber,
                p2BranchCurrentAlarmStatus   
            }
            STATUS              current
            DESCRIPTION         "Branch Current Alarm Status Bit Map: 0 HighHighLatching, 1 HighLatching, 2 LowLatching, 3 LowLowLatching, 4 AlarmOff, 8 HighNonLatching, 9 LowNonLatching"
            ::= { p2Traps 5 }

