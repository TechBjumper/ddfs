            OBJECTS
            {
                p1AuxAlarmPhase,
                p1AuxCurrentAlarmStatus  
            }
            STATUS              current
            DESCRIPTION         "Auxiliary Current Alarm Status Bit Map: 0 HighHighLatching, 1 HighLatching, 2 LowLatching, 3 LowLowLatching, 4 AlarmOff, 8 HighNonLatching, 9 LowNonLatching"
            ::= { p1Traps 4 }
    
