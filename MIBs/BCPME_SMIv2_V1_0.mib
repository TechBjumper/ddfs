                                clearAllMaxCurrentKW   (29877),
                                resetAllLatchingAlarms (31010)
                            }
        MAX-ACCESS          read-write
        STATUS              current
        DESCRIPTION         "Global Panel Resets. Write Codes: 26012 = Begin new Demand Sub-interval, 26013 = Reset Demand, 31010 = Reset all Latching Alarms, 10203 = Clear all KWH values to zero, 29877 = Clear all Max Current and Max KW values to zero, 20097 = Clear all Max Demand values to zero"
        ::= { p1Configuration 2 }


    -- Panel Setup

    p1Setup OBJECT IDENTIFIER ::= { p1Configuration 3 } 
      
        p1PanelConfiguration   OBJECT-TYPE    -- 
            SYNTAX              INTEGER
                                {
                                    p1PanelConfigurationTopFeedDoubleRow    (0),
                                    p1PanelConfigurationBottomFeedDoubleRow (1),
                                    p1PanelConfigurationSingleRowSequential (2),
                                    p1PanelConfigurationSingleRowOddEven    (3)
                                }  
            MAX-ACCESS          read-write
            STATUS              current
            DESCRIPTION         "Panel Configuration Bit Map. bit 0: 0 = normal / sequential, 1 = rotated / odd-even; bit 1: 0 = double-row, 1 = single-row.  Integer values:  0 = Top Feed, 1 = Bottom Feed, 2 = Single Row Sequential, 3 = Single Row Odd/Even."
            ::= { p1Setup 1 }

        p1NumSubIntervalsPerDemandInterval   OBJECT-TYPE    -- 
            SYNTAX              Unsigned32  (1..6)
            MAX-ACCESS          read-write
            STATUS              current
            DESCRIPTION         "Number of Sub-Intervals per Demand Interval. Sets the number of sub-intervals that make up a single demand interval. For block demand, set this to 1."
            ::= { p1Setup 2 }

        p1SubIntervalSeconds   OBJECT-TYPE    -- 
            SYNTAX              Unsigned32   (0..32767)
            MAX-ACCESS          read-write
            STATUS              current
            DESCRIPTION         "Sub-Interval Length in seconds. For sync-to-comms, set this to 0. Allowed lengths are 10-32767."
            ::= { p1Setup 3 }

        p1UserDefinedSettings  OBJECT-TYPE    -- 
            SYNTAX              BITS 
                                {
                                    p1EnableUserCTtoVoltagePhaseAssignment (0),  -- lets user assign branches to different voltage phases
                                    p1CurrentAngleVoltagePhaseRelative     (1),  -- otherwise angle is relative to V1 if False
                                    p1EnableSignedPower                    (2),  -- split core only
                                    p1EnableSignedPowerFactor              (3),  -- sign denotes lead lag
                                    p1EnableLogicalCircuitsByTypeArrays       (4),  -- disables legacy static 2 and 3 phase tables 
                                    p1DisablePanel2                        (5),  -- frees 2nd modbus address
                                    p1EnableVector2PhaseVAandPFcalc        (6),  -- default is simple VA calculation
                                    p1EnableRTCDemandSync                  (7)  -- default is free running demand timer
                                }
            MAX-ACCESS          read-write
            STATUS              current
            DESCRIPTION         "User Defined Settings Bit Map: 
                                 Bit 0 - Enable User CT to Voltage Phase Assignment
                                 Bit 1 - Enable Current Phase Angle Relative to associated Voltage phase (0 = V1 Relative)
                                 Bit 2 - Enable Signed Power (Split-Core Only)
                                 Bit 3 - Enable Signed Power Factor (sign denotes lead/lag)
                                 Bit 4 - Enable Flex Circuits By Data Type Arrays (Disables 2PH/3PH Pointmap)
                                 Bit 5 - Disable Second Panel (Disables 2nd Modbus address)
                                 Bit 6 - Enable Trig. Identity for 2PH KVA/PF Calculations (disables simple VA calculation)
                                 Bit 7 - Enable RTC for Demand Sync (disables free running demand timer)
                                 Bits 8-15  Reserved "
            ::= { p1Setup 4 }


