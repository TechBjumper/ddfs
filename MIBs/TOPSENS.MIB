TOPSENS-MIB DEFINITIONS ::= BEGIN

    IMPORTS 
        enterprises     FROM RFC1155-SMI
        OBJECT-TYPE     FROM RFC-1212
        DisplayString   FROM RFC1213-MIB
        TRAP-TYPE       FROM RFC-1215
        Gauge           FROM RFC1155-SMI
        Opaque          FROM RFC1155-SMI
        TimeTicks       FROM RFC1155-SMI;

    wagner      OBJECT IDENTIFIER ::= { enterprises 34187 }
    topsens     OBJECT IDENTIFIER ::= { wagner      74195}
    topsenstrap OBJECT IDENTIFIER ::= { topsens  3}

    ----------------------------------------------------
    -- Traps
    ----------------------------------------------------

   pswMainAlarmD1-trap   TRAP-TYPE
        ENTERPRISE topsenstrap
        VARIABLES { pswMainAlarmD1 }
        DESCRIPTION
            "Detector 1 fire alarm state changed."
        ::= 1000

    pswPreAlarmD1-trap   TRAP-TYPE
        ENTERPRISE topsenstrap
        VARIABLES { pswPreAlarmD1 }
        DESCRIPTION
            "Detector 1 action alarm state changed."
        ::= 1001

    pswInfAlarmD1-trap   TRAP-TYPE
        ENTERPRISE topsenstrap
        VARIABLES { pswInfAlarmD1 }
        DESCRIPTION
            "Detector 1 alert alarm state changed."
        ::= 1002

    pswMainAlarmD2-trap   TRAP-TYPE
        ENTERPRISE topsenstrap
        VARIABLES { pswMainAlarmD2 }
        DESCRIPTION
            "Detector 2 fire alarm state changed."
        ::= 1003

    pswPreAlarmD2-trap   TRAP-TYPE
        ENTERPRISE topsenstrap
        VARIABLES { pswPreAlarmD2 }
        DESCRIPTION
            "Detector 2 action alarm state changed."
        ::= 1004

    pswInfAlarmD2-trap   TRAP-TYPE
        ENTERPRISE topsenstrap
        VARIABLES { pswInfAlarmD2 }
        DESCRIPTION
            "Detector 2 alert alarm state changed."
        ::= 1005

    pswFailureDisplayOn-trap   TRAP-TYPE
        ENTERPRISE topsenstrap
        VARIABLES {pswFailureDisplayOn  }
        DESCRIPTION
            "Failure display state changed."
        ::= 1006

    pswAlternateSensitivityOn-trap   TRAP-TYPE
        ENTERPRISE topsenstrap
        VARIABLES { pswAlternateSensitivityOn }
        DESCRIPTION
            "Sensitivity changed."
        ::= 1007

    pswAirFlowInitD1-trap   TRAP-TYPE
        ENTERPRISE topsenstrap
        VARIABLES {pswAirFlowInitD1  }
        DESCRIPTION
            "Air flow initialization state of detector 1 changed."
        ::= 1008

    pswAirFlowInitD2-trap   TRAP-TYPE
        ENTERPRISE topsenstrap
        VARIABLES {pswAirFlowInitD2  }
        DESCRIPTION
            "Air flow initialization state of detector 2 changed."
        ::= 1009

--  pswBit10-trap   TRAP-TYPE
--      ENTERPRISE topsenstrap
--      VARIABLES {pswBit10 }
--      DESCRIPTION
--          "Normally unused state variable (bit 10) changed."
--      ::= 1010

--  pswBit11-trap   TRAP-TYPE
--      ENTERPRISE topsenstrap
--      VARIABLES { pswBit11  }
--      DESCRIPTION
--          "Normally unused state variable (bit 11) changed."
--      ::= 1011

    pswStoredMessagesResetD1-trap   TRAP-TYPE
        ENTERPRISE topsenstrap
        VARIABLES { pswStoredMessagesResetD1 }
        DESCRIPTION
            "Detector 1 stored messages reset / changed."
        ::= 1012

    pswStoredMessagesResetD2-trap   TRAP-TYPE
        ENTERPRISE topsenstrap
        VARIABLES { pswStoredMessagesResetD2 }
        DESCRIPTION
            "Detector 2 stored messages reset / changed."
        ::= 1013

    pswStoredMessagesResetUnit-trap   TRAP-TYPE
        ENTERPRISE topsenstrap
        VARIABLES { pswStoredMessagesResetUnit  }
        DESCRIPTION
            "Main board stored messages reset / changed."
        ::= 1014

--  pswBit15-trap   TRAP-TYPE
--      ENTERPRISE topsenstrap
--      VARIABLES { pswBit15 }
--      DESCRIPTION
--          "Normally unused state variable (bit 15) changed."
--      ::= 1015

    ----------------------------------------------------
    ----------------------------------------------------

    basicTable OBJECT-TYPE
        SYNTAX SEQUENCE OF BasicEntry
        ACCESS not-accessible
        STATUS mandatory
        ::= { topsens 1 }

    basicEntry OBJECT-TYPE
        SYNTAX BasicEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
            "This is the table for basic entries!"
        INDEX { basicIndex }
        ::= { basicTable 1 }

    BasicEntry ::= SEQUENCE {
        basicIndex                  INTEGER,
        tsManufacturer              DisplayString,
        tsUnitName                  DisplayString,
        tsUnitVersion               DisplayString,
        tsObjectTableLoadingState   INTEGER,
        tsObjectTableEntries        INTEGER,
        tsObjectDataValid           INTEGER,
        tsProcessData               OCTET STRING,
        
        -- Process data bits
        
        pswMainAlarmD1              INTEGER,
        pswPreAlarmD1               INTEGER,
        pswInfAlarmD1               INTEGER,
        pswMainAlarmD2              INTEGER,
        pswPreAlarmD2               INTEGER,
        pswInfAlarmD2               INTEGER,
        pswFailureDisplayOn         INTEGER,
        pswAlternateSensitivityOn   INTEGER,
        pswAirFlowInitD1            INTEGER,
        pswAirFlowInitD2            INTEGER,
--      pswUnusedBit10              INTEGER,
--      pswUnusedBit11              INTEGER,
        pswStoredMessagesResetD1    INTEGER,
        pswStoredMessagesResetD2    INTEGER,
        pswStoredMessagesResetUnit  INTEGER
--      pswUnusedBit15              INTEGER
    }

    ----------------------------------------------------
    -- TopSens basic data
    ----------------------------------------------------

    basicIndex OBJECT-TYPE
        SYNTAX  INTEGER (0..1)
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "A unique value for each table."
        ::= { basicEntry 99 } 

    tsManufacturer OBJECT-TYPE
        SYNTAX DisplayString
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The name of the TopSens manufacturer."
        ::= { basicEntry  1}

    tsUnitName OBJECT-TYPE
        SYNTAX DisplayString
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The name of the TopSens unit."
        ::= { basicEntry 2}

    tsUnitVersion OBJECT-TYPE
        SYNTAX DisplayString
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The version of the TopSens object table."
        ::= { basicEntry 3}

    tsObjectTableLoadingState OBJECT-TYPE
        SYNTAX INTEGER {
          notloaded(0),
          loading(1),
          loaded(2)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The loading state of object table.
             0=table in not loaded
             1=table loading in process
             2=table is loaded"
        ::= { basicEntry 4}

    tsObjectTableEntries OBJECT-TYPE
        SYNTAX INTEGER 
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The no. of objects in the object table."
        ::= { basicEntry 5}

    tsObjectDataValid OBJECT-TYPE
        SYNTAX INTEGER {
          valid(0),
          invalid(1)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The marker for invalid object data, set when object data is invalid."
        ::= { basicEntry 6}

    tsProcessData OBJECT-TYPE
        SYNTAX OCTET STRING
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The actual state of the TopSens. It consists of 16 bit from which every
             one of them can be examined under id 1000 to 1015."
        ::= { basicEntry 100 }

    ----------------------------------------------------
    -- Bits from TopSens Process Data
    ----------------------------------------------------

    pswMainAlarmD1 OBJECT-TYPE
        SYNTAX INTEGER {
            idle(0),
            alarm(1)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Detector 1 fire alarm state.
             It is one (1) if the fire alarm is released."
        ::= { basicEntry 1000 }

    pswPreAlarmD1 OBJECT-TYPE
        SYNTAX INTEGER {
            idle(0),
            alarm(1)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Detector 1 action alarm state.
             It is one (1) if the action alarm is released."
        ::= { basicEntry 1001 }

    pswInfAlarmD1 OBJECT-TYPE
        SYNTAX INTEGER {
            idle(0),
            alarm(1)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Detector 1 alert alarm state.
             It is one (1) if the alert alarm is released."
        ::= { basicEntry 1002 }

    pswMainAlarmD2 OBJECT-TYPE
        SYNTAX INTEGER {
            idle(0),
            alarm(1)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Detector 2 fire alarm state.
             It is one (1) if the fire alarm is released."
        ::= { basicEntry 1003 }

    pswPreAlarmD2 OBJECT-TYPE
        SYNTAX INTEGER {
            idle(0),
            alarm(1)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Detector 2 action alarm state.
             It is one (1) if the action alarm is released."
        ::= { basicEntry 1004 }

    pswInfAlarmD2 OBJECT-TYPE
        SYNTAX INTEGER {
            idle(0),
            alarm(1)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Detector 2 alert alarm state.
             It is one (1) if the alert alarm is released."
        ::= { basicEntry 1005 }

    pswFailureDisplayOn OBJECT-TYPE
        SYNTAX INTEGER {
            off(0),
            on(1)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "If one (1) the failure display is on."
        ::= { basicEntry 1006 }

    pswAlternateSensitivityOn OBJECT-TYPE
        SYNTAX INTEGER {
            off(0),
            on(1)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "If one (1) the alternative sensitivity is switched on."
        ::= { basicEntry 1007 }

    pswAirFlowInitD1 OBJECT-TYPE
        SYNTAX INTEGER {
            off(0),
            on(1)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "If one (1) the detector 1 is in air flow initialization state."
        ::= { basicEntry 1008 }

    pswAirFlowInitD2 OBJECT-TYPE
        SYNTAX INTEGER {
            off(0),
            on(1)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "If one (1) the detector 2 is in air flow initialization state."
        ::= { basicEntry 1009 }

