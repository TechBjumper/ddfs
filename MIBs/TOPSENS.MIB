    ----------------------------------------------------
    -- TopSens object data
    ----------------------------------------------------

    objectIndex OBJECT-TYPE
        SYNTAX  INTEGER (0..1)
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
                "A unique value for each table."
        ::= { objectEntry 99 } 

    tsUnitState                     OBJECT-TYPE 
        SYNTAX OCTET STRING 
        ACCESS read-only 
        STATUS mandatory 
        DESCRIPTION 
            "The actual state of the TopSens. It consists of 16 bit from which every
             one of them can be examined under id 24576000 to 24576015."
        ::= { objectEntry 24576 }
        
    tsUnitSoftwareNumber            OBJECT-TYPE 
        SYNTAX DisplayString
        ACCESS read-only 
        STATUS mandatory 
        DESCRIPTION 
            "The software number of the firmware in the mainboard of the TopSens."
        ::= { objectEntry 24577 }
        
    tsNumberOfDetectors             OBJECT-TYPE 
        SYNTAX INTEGER      
        ACCESS read-only 
