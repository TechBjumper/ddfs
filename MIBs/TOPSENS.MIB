--      SYNTAX INTEGER { off(0), on(1) }
--      ACCESS read-only
--      STATUS mandatory
--      DESCRIPTION
--          "Unused bit 11."
--      ::= { basicEntry 1011 }

    pswStoredMessagesResetD1 OBJECT-TYPE
        SYNTAX INTEGER {
            idle(0),
            reset(1)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "If one (1) the stored messages in detector 1 are reset."
        ::= { basicEntry 1012 }

    pswStoredMessagesResetD2 OBJECT-TYPE
        SYNTAX INTEGER {
            idle(0),
            reset(1)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "If one (1) the stored messages in detector 2 are reset."
        ::= { basicEntry 1013 }

    pswStoredMessagesResetUnit OBJECT-TYPE
        SYNTAX INTEGER {
            idle(0),
            reset(1)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "If one (1) the stored messages on the unit mainboard are reset."
        ::= { basicEntry 1014 }

--  pswUnusedBit15 OBJECT-TYPE
--      SYNTAX INTEGER { off(0), on(1) }
--      ACCESS read-only
--      STATUS mandatory
--      DESCRIPTION
--          "Unused bit 15."
--      ::= { basicEntry 1015 }

