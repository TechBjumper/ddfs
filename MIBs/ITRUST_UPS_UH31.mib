    upsSmartConfigReturnDelay OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "The delay in seconds after utility line power returns
             before the UPS will turn on.  This value is also used
             when the UPS comes out of a reboot and before the UPS
             wakes up from 'sleep' mode.  Allowed values are from 
             0 to 43200 seconds(12 hours).
    
             If a value other than a supported value is provided in a
             set request, the UPS interprets it as a the next higher
             acceptable value.  If the provided value is higher than
             the highest acceptable value, the highest acceptable
             value is used."
        ::= { upsSmartConfig 1 }
    
    upsSmartConfigShutoffDelay OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "The delay in seconds the UPS remains on after being told
             to turn off.  Allowed values are from 120 to 600 seconds.
    
             If a value other than a supported value is provided in a
             set request, the UPS interprets it as next higher acceptable
             value.  If the provided value is higher than the highest
             acceptable value, the highest acceptable value is used."
        ::= { upsSmartConfig 2 }
    
    
    upsSmartConfigAutoReturn OBJECT-TYPE
        SYNTAX INTEGER  {
            unknown(1),
            no(2),
            yes(3)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "If yes(3) the UPS will auto return after utility line 
             power returns, no(2) to keep off.
             a unknown(1) return indicates an error occurred on 
             getting this config variable."
        ::= { upsSmartConfig 3 }

    upsSmartConfigTimeOfBatteryDiagnostic OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "The time of battery self diagnostic in seconds,
            the valid value is 1 to 600 seconds." 
        ::= { upsSmartConfig 4 }

    upsSmartConfigCombiningMode OBJECT-TYPE
        SYNTAX INTEGER {
            unknown(1),
            single(2),
            master(3),
            slave(4)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Setting this variable to indicate the combining work mode of the UPS."
        ::= { upsSmartConfig 5 }
 
    upsSmartConfigCombiningWorkTime OBJECT-TYPE
        SYNTAX INTEGER {
            unknown(1),
            by-7-days(2),          -- oneWeek
            by-30-days(3),         -- oneMonth
            by-90-days(4),         -- oneSeason
            by-180-days(5)         -- halfYear
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "The work time of alternating from master UPS and slave or
             from slave to master."
        ::= { upsSmartConfig 6 }
    
    upsSmartConfigWorkMode OBJECT-TYPE
        SYNTAX INTEGER {
            unknown(1),
            normal(2),
            eco(3) 
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "The work mode of UH31 series UPS. 
            eco(3) set UPS work in a economic mode."
        ::= { upsSmartConfig 7 }

    upsSmartConfigAlaramRelay OBJECT-TYPE
        SYNTAX INTEGER {
            unknown(1),
            batteryFail(2),
            chargerFail(3),
            utilityFail(4),
            phasesLost(5),
            noZeroPhase(6),
            bypassFail(7),
            overLoad(8)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Set the condition to act alarm relay. the condition is one of
             the following event happened:
             batteryFail(2), battery failure,
             chargerFail(3), charger failure,
             utilityFail(4), utility failure,
             phasesLost(5), one or more input phases are lost,
             noZeroPhase(6), the zero phase of utility is lost,
             bypassFail(7), bypass failure,
             overLoad(8), the output is overload."
        ::= { upsSmartConfig 8 }

	upsSmartConfigEnablePowerSupplyTimeOnBattery OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "The time of enable power supply on battery in seconds,
            the valid value is -1 to 86400 seconds. 
			the UPS will be delayed shutdown if the time out.
			if -1 set to this variable, UPS will supply power till
			the battery low or utility power restore. " 
        ::= { upsSmartConfig 9 }


