        ::= { upsPlanConfigEntry 2 }

    scheduleCycle OBJECT-TYPE
        SYNTAX INTEGER  {
            once(1),
            daily(2),
            weekly(3),
            biweekly(4),
            monthly(5)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
