    -- ------------------------------------------------------------------------ 
    -- 3.2.2.1 the upsBaseBattery group
    -- ------------------------------------------------------------------------ 
    upsBaseBatteryStatusOfBattery1 OBJECT-TYPE
       SYNTAX INTEGER  {
            unknown(1),
            batteryNormal(2),
            batteryLow(3),
            batteryHigh(4), 
            batteryFault(5), 
            batteryForewarning(6)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The status of No.1 battery. A batteryLow(3)
             value indicates the UPS will be unable to sustain the
             current load, and its services will be lost if power is
             not restored.  The amount of run time in reserve at the
             time of low battery can be configured by the
             upsSmartConfigLowBatteryRunTime. A batteryFault(5) indicates
             the battery has some faults, batteryForewarning(6) indicates
             the battery is forewarning."
        ::= { upsBaseBattery 1 }
    
    upsBaseBatteryStatusOfBattery2 OBJECT-TYPE
       SYNTAX INTEGER  {
            unknown(1),
            batteryNormal(2),
            batteryLow(3),
            batteryHigh(4), 
            batteryFault(5), 
            batteryForewarning(6)
        }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The status of No.2 battery.  A batteryLow(3)
             value indicates the UPS will be unable to sustain the
             current load, and its services will be lost if power is
             not restored.  The amount of run time in reserve at the
             time of low battery can be configured by the
             upsSmartConfigLowBatteryRunTime. A batteryFault(5) indicates
             the battery has some faults, batteryForewarning(6) indicates
             the battery is forewarning."
        ::= { upsBaseBattery 2 }

