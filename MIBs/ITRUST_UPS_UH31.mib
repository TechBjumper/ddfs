    upsIdent        OBJECT IDENTIFIER ::= { upsCommon 1 }
    
    iTrustUH11      OBJECT IDENTIFIER ::= { upsSpecial 1 }
    iTrustUH31      OBJECT IDENTIFIER ::= { upsSpecial 2 }
    iTrustUL33      OBJECT IDENTIFIER ::= { upsSpecial 3 }

