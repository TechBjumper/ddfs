PhaseRotation ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "description"
    SYNTAX      INTEGER {
            phaseRotationABC(1), phaseRotationCBA(2) }

systemName OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(0..40))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
