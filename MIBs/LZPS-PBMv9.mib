            system415LineNeutral(7), system480LineNeutral(8), system600LineNeutral(9),
            system380LineLine(10), system400LineLine(11), system415LineLine(12),
            system480LineLine(13), system600LineLine(14) }

BooleanDataType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "description"
    SYNTAX      INTEGER {
            booleanFalse(1), booleanTrue(2) }

AlarmDataType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "description"
    SYNTAX      INTEGER {
            alarmNormal(1), alarmActive(2), alarmUnknown(3) }

BreakerState ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "description"
    SYNTAX      INTEGER {
            breakerStateOpen(1), breakerStateClosed(2) }

TriggerDataType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "description"
