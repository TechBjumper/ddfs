		<stateSensor ruleid="device_in_1_stand_by">
			<type>state</type>
			<sensorId>_DEVICE_IN_1_STAND_BY</sensorId>
			<value>
				<mapValue mapid="truefalse_vm">
					<formatNumber printffmt="%1.0f">
						<bitwise_and>
							<op>
								<right_shift>
									<op>
										<getOid>DEVICE_IN_1_STAND_BY</getOid>
									</op>
									<op>2</op>
								</right_shift>
							</op>
							<op>1</op>
						</bitwise_and>
					</formatNumber>
				</mapValue>
			</value>
			<label>Device IN 1 Stand By</label>
			<enum>truefalse_em</enum>
			<sensorSet>Status Sensors</sensorSet>
		</stateSensor>
