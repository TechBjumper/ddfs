	<!--Batt Alarms Batt #1-->
	
	<alarmFlags2 ruleid="batt_1_status-af">
			<value><getOid>.1.3.6.1.4.1.318.1.1.1.13.3.2.1.3.1</getOid></value>
			<alarmFlagsTest match="1">
				<name>
					<activeText>Battery #1 Status: an Unknown condition exists.</activeText>
					<inactiveText>Battery #1 Status Normal</inactiveText>
				</name>
				<severity>failure</severity>
				<category>System</category>
				<description>
					<activeText>Battery #1 Status: an Unknown condition exists.</activeText>
					<inactiveText>Battery #1 Status has returned to normal.</inactiveText>
				</description>
			</alarmFlagsTest>
			<alarmFlagsTest match="4">
