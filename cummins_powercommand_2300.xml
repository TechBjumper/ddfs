<?xml version="1.0"?>
<!-- Test Modbus related information -->
<APC_DDF xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://localhost/ddf_schema/apc_ddf_1_6.xsd"
	ddfid="cummins_powercommand_2300"
	ddfname="Cummins PowerCommand 2300/3300"
	ddfversion="5"
	canremove="yes">
  <schemaVersion>1.6</schemaVersion>

  <device deviceid="deepsea_generator_sensors">
    <oidMustExist ruleid="modbusdevicetest" oid="MODBUS_OID_MUST_EXIST"/>

    <setProductData ruleid="vendordata" field="vendor">Cummins</setProductData>

    <!-- PowerCommand Digital Controls used to monitor generators -->
    <setProductData ruleid="modeldata" field="model">PowerCommand 2300/3300</setProductData>

    <!-- Generator -->
    <setProductData ruleid="typedata" field="type">Generator</setProductData>

    <setDefaultLabel ruleid="labeldata"><sysHostname/> - Slave <commSetting field="slaveaddress"/></setDefaultLabel>

    <stateSensor ruleid="gen_0_engine_op_mode">
      <type>state</type>
      <sensorId>GEN0_GENERATOR_STATUS</sensorId>
      <value>
        <mapValue mapid="generatorstatus_valuemap">
          <getOid>GEN0_GENERATOR_STATUS</getOid>
        </mapValue>
      </value>
      <label>Control Switch Position</label>
      <enum>generatorstatus_enummap</enum>
      <sensorSet>Generator Sensors</sensorSet>
    </stateSensor>

    <valueMap ruleid="generatorstatus_valuemap">
      <valueIn>0</valueIn><valueOut>0</valueOut>
      <valueIn>1</valueIn><valueOut>1</valueOut>
      <valueIn>2</valueIn><valueOut>2</valueOut>
      <valueIn>*</valueIn><valueOut>3</valueOut>
    </valueMap>

    <enumMap ruleid="generatorstatus_enummap">
      <label>Off</label>
      <label>Auto</label>
      <label>Manual</label>
      <label>Unknown</label>
    </enumMap>

    <stateSensor ruleid="gen_0_devicestatus">
      <type>state</type>
      <sensorId>GEN0_DEVICE_STATUS</sensorId>
      <value>
        <mapValue mapid="devicestatus_valuemap">
          <getOid>GEN0_DEVICE_STATUS</getOid>
        </mapValue>
      </value>
      <label>Run Sequence State</label>
      <enum>devicestatus_enummap</enum>
      <sensorSet>Generator Sensors</sensorSet>
    </stateSensor>

    <valueMap ruleid="devicestatus_valuemap">
      <valueIn>0</valueIn><valueOut>1</valueOut>
      <valueIn>1</valueIn><valueOut>2</valueOut>
      <valueIn>2</valueIn><valueOut>3</valueOut>
      <valueIn>3</valueIn><valueOut>4</valueOut>
      <valueIn>4</valueIn><valueOut>5</valueOut>
      <valueIn>5</valueIn><valueOut>6</valueOut>
      <valueIn>6</valueIn><valueOut>7</valueOut>
      <valueIn>*</valueIn><valueOut>0</valueOut>
    </valueMap>

    <enumMap ruleid="devicestatus_enummap">
      <label>Unknown</label>
      <label>Stop</label>
      <label>Time Delay to Start</label>
      <label>Warmup at Idle</label>
      <label>Rated Freq and Voltage</label>
      <label>Cooldown/Stop Delay</label>
      <label>Cooldown at Idle</label>
      <label>Rated to Idle Transition Delay</label>
    </enumMap>


    <stateSensor ruleid="gen_0_remote_start_switch">
      <type>state</type>
      <sensorId>GEN0_REMOTE_START_SWITCH_STATUS</sensorId>
      <value>
        <mapValue mapid="inactiveactive_valuemap">
          <getOid>GEN0_REMOTE_START_SWITCH_STATUS</getOid>
        </mapValue>
      </value>
      <label>Remote Start Switch</label>
      <enum>inactiveactive_enummap</enum>
      <sensorSet>Generator Sensors</sensorSet>
    </stateSensor>

    <stateSensor ruleid="gen_0_common_fault_status">
      <type>state</type>
      <sensorId>GEN0_COMMON_FAULT_STATUS</sensorId>
      <value>
        <mapValue mapid="inactiveactive_valuemap">
          <getOid>GEN0_COMMON_FAULT_STATUS</getOid>
        </mapValue>
      </value>
      <label>Common Alarm Fault Status</label>
      <enum>inactiveactive_enummap</enum>
      <sensorSet>Generator Sensors</sensorSet>
    </stateSensor>

    <stateSensor ruleid="gen_0_common_shutdown_cmd">
      <type>state</type>
      <sensorId>GEN0_COMMON_SHUTDOWN_CMD</sensorId>
      <value>
        <mapValue mapid="inactiveactive_valuemap">
          <getOid>GEN0_COMMON_SHUTDOWN_CMD</getOid>
        </mapValue>
      </value>
      <label>Common Shutdown Command</label>
      <enum>inactiveactive_enummap</enum>
      <sensorSet>Generator Sensors</sensorSet>
    </stateSensor>

    <valueMap ruleid="inactiveactive_valuemap">
      <valueIn>0</valueIn><valueOut>0</valueOut>
      <valueIn>1</valueIn><valueOut>1</valueOut>
      <valueIn>*</valueIn><valueOut>2</valueOut>
    </valueMap>

    <enumMap ruleid="inactiveactive_enummap">
      <label>Inactive</label>
      <label>Active</label>
      <label>Unknown</label>
    </enumMap>

    <stateSensor ruleid="gen_0_crankselect">
      <type>state</type>
      <sensorId>GEN0_CRANK_STATUS</sensorId>
      <value>
        <mapValue mapid="crankstatus_valuemap">
          <getOid>GEN0_CRANK_STATUS</getOid>
        </mapValue>
      </value>
      <label>Crank Select Mode</label>
      <enum>crankstatus_enummap</enum>
      <sensorSet>Generator Sensors</sensorSet>
    </stateSensor>

    <valueMap ruleid="crankstatus_valuemap">
      <valueIn>0</valueIn><valueOut>0</valueOut>
      <valueIn>1</valueIn><valueOut>1</valueOut>
      <valueIn>*</valueIn><valueOut>2</valueOut>
    </valueMap>

    <enumMap ruleid="crankstatus_enummap">
      <label>Cycle</label>
      <label>Continuous</label>
      <label>Unknown</label>
    </enumMap>

    <numSensor ruleid="voltageln" index="voltageln_table">
      <type>voltage</type>
      <sensorId><rowIndex/></sensorId>
      <value><getRowOid>voltageln_table</getRowOid></value>
      <label><mapValue mapid="phase_voltageln_sensorlabel"><rowNumber/></mapValue><space/>Voltage</label>
      <sensorSet>Generator Sensors</sensorSet>
    </numSensor>

    <valueMap ruleid="phase_voltageln_sensorlabel">
      <valueIn>0</valueIn><valueOut>L1N</valueOut>
      <valueIn>1</valueIn><valueOut>L2N</valueOut>
      <valueIn>2</valueIn><valueOut>L3N</valueOut>

      <valueIn>*</valueIn><valueOut>*</valueOut>
    </valueMap>

    <numSensor ruleid="voltagell" index="voltagell_table">
      <type>voltage</type>
      <sensorId><rowIndex/></sensorId>
      <value><getRowOid>voltagell_table</getRowOid></value>
      <label><mapValue mapid="phase_voltagell_sensorlabel"><rowNumber/></mapValue><space/>Voltage</label>
      <sensorSet>Generator Sensors</sensorSet>
    </numSensor>

    <valueMap ruleid="phase_voltagell_sensorlabel">
      <valueIn>0</valueIn><valueOut>L1L2</valueOut>
      <valueIn>1</valueIn><valueOut>L2L3</valueOut>
      <valueIn>2</valueIn><valueOut>L3L1</valueOut>
      <valueIn>3</valueIn><valueOut>Nominal</valueOut>

      <valueIn>*</valueIn><valueOut>*</valueOut>
    </valueMap>

    <numSensor ruleid="current" index="current_table">
      <type>amperage</type>
      <sensorId><rowIndex/></sensorId>
      <value><getRowOid>current_table</getRowOid></value>
      <label><mapValue mapid="phase_current_sensorlabel"><rowNumber/></mapValue><space/>Current</label>
      <sensorSet>Generator Sensors</sensorSet>
    </numSensor>

    <valueMap ruleid="phase_current_sensorlabel">
      <valueIn>0</valueIn><valueOut>L1</valueOut>
      <valueIn>1</valueIn><valueOut>L2</valueOut>
      <valueIn>2</valueIn><valueOut>L3</valueOut>

      <valueIn>*</valueIn><valueOut>*</valueOut>
    </valueMap>

    <numSensor ruleid="realpower" index="realpower_table">
      <type>num/powerKW</type>
      <sensorId>KW_<rowIndex/></sensorId>
      <value><getRowOid>realpower_table</getRowOid></value>
      <label><mapValue mapid="phase_power_sensorlabel"><rowNumber/></mapValue><space/>Real Power</label>
      <sensorSet>Generator Sensors</sensorSet>
    </numSensor>

    <numSensor ruleid="apparentpower" index="apparentpower_table">
      <type>num/powerKVA</type>
      <sensorId>KVA_<rowIndex/></sensorId>
      <value><getRowOid>apparentpower_table</getRowOid></value>
      <label><mapValue mapid="phase_power_sensorlabel"><rowNumber/></mapValue><space/>Apparent Power</label>
      <sensorSet>Generator Sensors</sensorSet>
    </numSensor>

    <numSensor ruleid="reactivepower" index="reactivepower_table">
      <type>num/powerKVAR</type>
      <sensorId>KVAR_<rowIndex/></sensorId>
      <value><getRowOid>reactivepower_table</getRowOid></value>
      <label><mapValue mapid="phase_power_sensorlabel"><rowNumber/></mapValue><space/>Reactive Power</label>
      <sensorSet>Generator Sensors</sensorSet>
    </numSensor>

    <numSensor ruleid="powerfactor" index="powerfactor_table">
      <type>num/powerfactor</type>
      <sensorId><rowIndex/></sensorId>
      <value>
        <mult> <!-- shift to hundreds -->
          <op><getRowOid>powerfactor_table</getRowOid></op>
          <op>0.01</op>
        </mult>
      </value>
      <minValue>-1.0</minValue>
      <valueInc>0.01</valueInc>
      <label><mapValue mapid="phase_power_sensorlabel"><rowNumber/></mapValue><space/>Power Factor</label>
      <sensorSet>Generator Sensors</sensorSet>
    </numSensor>

    <valueMap ruleid="phase_power_sensorlabel">
      <valueIn>0</valueIn><valueOut>Total</valueOut>
      <valueIn>1</valueIn><valueOut>L1</valueOut>
      <valueIn>2</valueIn><valueOut>L2</valueOut>
      <valueIn>3</valueIn><valueOut>L3</valueOut>

      <valueIn>*</valueIn><valueOut>*</valueOut>
    </valueMap>

    <numSensor ruleid="frequency">
      <type>frequency</type>
      <sensorId>GEN0_FREQUENCY_1</sensorId>
      <value>
        <mult> <!-- shift to tenths -->
          <op><getOid>GEN0_FREQUENCY_1</getOid></op>
          <op>0.1</op>
        </mult>
      </value>
      <label>Frequency</label>
      <sensorSet>Generator Sensors</sensorSet>
    </numSensor>

    <numSensor ruleid="realenergy">
      <type>num/kwatthr</type>
      <sensorId>GEN0_TOTAL_ENERGY_KWH_1</sensorId>
      <value><getOid>GEN0_TOTAL_ENERGY_KWH_1</getOid></value>
      <label>Total Net Real Energy</label>
      <sensorSet>Generator Sensors</sensorSet>
    </numSensor>




    <numSensor ruleid="gen_0_voltage_battery">
      <type>voltage</type>
      <sensorId>GEN0_BATT_VOLTAGE</sensorId>
      <value>
        <mult>
          <op><getOid>GEN0_BATT_VOLTAGE</getOid></op>
          <op>0.1</op>
        </mult>
      </value>
      <label>Battery Voltage</label>
      <sensorSet>Generator Engine Sensors</sensorSet>
    </numSensor>

    <numSensor ruleid="gen_0_engine_oil_pressure">
      <type>pressure</type>
      <sensorId>GEN0_OIL_PRESSURE</sensorId>
      <value>
        <mult>
          <op><getOid>GEN0_OIL_PRESSURE</getOid></op>
          <op>0.1</op>
        </mult>
      </value>
      <label>Oil Pressure</label>
      <sensorSet>Generator Engine Sensors</sensorSet>
    </numSensor>

    <numSensor ruleid="gen_0_engine_coolant_temp">
      <type>temp</type>
      <sensorId>GEN0_ENGINE_COOLANT_TEMP</sensorId>
      <value>
        <mult>
          <op><getOid>GEN0_ENGINE_COOLANT_TEMP</getOid></op>
          <op>0.1</op>
        </mult>
      </value>
      <minValue>-50.0</minValue>
      <maxValue>200.0</maxValue>
      <label>Coolant Temperature</label>
      <sensorSet>Generator Engine Sensors</sensorSet>
    </numSensor>

    <numSensor ruleid="gen_0_engine_speed">
      <type>fanspeed</type>
      <sensorId>GEN0_ENGINE_SPEED</sensorId>
      <value><getOid>GEN0_ENGINE_SPEED</getOid></value>
      <label>Average Engine Speed</label>
      <sensorSet>Generator Engine Sensors</sensorSet>
    </numSensor>

    <numSensor ruleid="gen_0_engine_runhours">
      <type>runhours</type>
      <sensorId>GEN0_ENGINE_RUNHOURS</sensorId>
      <value>
        <div> <!-- shift seconds to hours -->
          <op><getOid>GEN0_ENGINE_RUNHOURS</getOid></op>
          <op>36000</op>
        </div>
      </value>
      <valueInc>0.01</valueInc>
      <label>Engine Running Time</label>
      <sensorSet>Generator Engine Sensors</sensorSet>
    </numSensor>

    <numSensor ruleid="gen_0_engine_starts">
      <type>num</type>
      <sensorId>GEN0_ENGINE_STARTS</sensorId>
      <value><getOid>GEN0_ENGINE_STARTS</getOid></value>
      <label>Total Number of Runs</label>
      <sensorSet>Generator Engine Sensors</sensorSet>
    </numSensor>

    <numSensor ruleid="gen_0_engine_fuel_level">
      <type>pctofcapacity</type>
      <sensorId>GEN0_FUELTANK_LEVEL</sensorId>
      <value>
        <mult>
          <op><getOid>GEN0_FUELTANK_LEVEL</getOid></op>
          <op>0.1</op>
        </mult>
      </value>
      <minValue>0.0</minValue>
      <maxValue>130.0</maxValue>
      <label>Fuel Level Percent</label>
      <sensorSet>Generator Engine Sensors</sensorSet>
    </numSensor>

    <numSensor ruleid="gen_0_fuel_tot">
      <type>num</type>
      <units>liters</units>
      <sensorId>GEN0_TOTAL_FUEL</sensorId>
      <value>
        <mult> <!-- gallons to liters -->
          <op><getOid>GEN0_TOTAL_FUEL</getOid></op>
          <op>3.785412</op>
        </mult>
      </value>
      <valueInc>0.01</valueInc>
      <label>Fuel Level</label>
      <sensorSet>Generator Engine Sensors</sensorSet>
    </numSensor>
    <numSensor ruleid="gen_0_fuel_rate">
      <type>num</type>
      <sensorId>GEN0_FUEL_RATE</sensorId>
      <value>
        <mult><!-- gal/hr to L/hr -->
          <op><getOid>GEN0_FUEL_RATE</getOid></op>
          <op>0.1892706</op>
        </mult>
      </value>
      <valueInc>0.001</valueInc>
      <label>Fuel Rate (L/hr)</label>
      <sensorSet>Generator Engine Sensors</sensorSet>
    </numSensor>

    <numSensor ruleid="gen_0_mostRecentFault">
      <type>num</type>
      <sensorId>GEN0_MOST_RECENT_FAULT</sensorId>
      <value>
		<getOid>GEN0_MOST_RECENT_FAULT</getOid>
      </value>
      <valueInc>1</valueInc>
      <label>Fault Code of Most Recent Fault or Warning</label>
      <sensorSet>Generator Engine Sensors</sensorSet>
    </numSensor>
    <numSensor ruleid="gen_0_standbyTotalKW">
      <type>pctofcapacity</type>
      <sensorId>GEN0_STANDBY_TOTAL_KW</sensorId>
      <value>
		<mult>
		<op><getOid>GEN0_STANDBY_TOTAL_KW</getOid></op>
		<op>0.01</op>
		</mult>
      </value>
      <valueInc>0.01</valueInc>
      <label>Standby Total kW</label>
      <sensorSet>Generator Engine Sensors</sensorSet>
    </numSensor>
    <numSensor ruleid="gen_0_LLAvgVoltage">
      <type>voltage</type>
      <sensorId>GEN0_LL_AVG_VOLTAGE</sensorId>
      <value>
		<mult>
		<op><getOid>GEN0_VOLTAGE_LL_AVG</getOid></op>
		<op>1</op>
		</mult>
      </value>
      <valueInc>1</valueInc>
      <label>LL Average Voltage</label>
      <sensorSet>Generator Engine Sensors</sensorSet>
    </numSensor>
    <numSensor ruleid="gen_0_avgCurrent">
      <type>amperage</type>
      <sensorId>GEN0_AVG_CURRENT</sensorId>
      <value>
		<mult>
		<op><getOid>GEN0_CURRENT_AVG</getOid></op>
		<op>1</op>
		</mult>
      </value>
      <valueInc>1</valueInc>
      <label>Average Current</label>
      <sensorSet>Generator Engine Sensors</sensorSet>
    </numSensor>
    <numSensor ruleid="gen_0_standbyCurrentL1">
      <type>pctofcapacity</type>
      <sensorId>GEN0_STANDBY_CURRENT_L1</sensorId>
      <value>
		<mult>
		<op><getOid>GEN0_STANDBY_CURRENT_L1</getOid></op>
		<op>0.1</op>
		</mult>
      </value>
      <valueInc>0.1</valueInc>
      <label>Standby L1 Current</label>
      <sensorSet>Generator Engine Sensors</sensorSet>
    </numSensor>
    <numSensor ruleid="gen_0_standbyCurrentL2">
      <type>pctofcapacity</type>
      <sensorId>GEN0_STANDBY_CURRENT_L2</sensorId>
      <value>
		<mult>
		<op><getOid>GEN0_STANDBY_CURRENT_L2</getOid></op>
		<op>0.1</op>
		</mult>
      </value>
      <valueInc>0.1</valueInc>
      <label>Standby L2 Current</label>
      <sensorSet>Generator Engine Sensors</sensorSet>
    </numSensor>
    <numSensor ruleid="gen_0_standbyCurrentL3">
      <type>pctofcapacity</type>
      <sensorId>GEN0_STANDBY_CURRENT_L3</sensorId>
      <value>
		<mult>
		<op><getOid>GEN0_STANDBY_CURRENT_L3</getOid></op>
		<op>0.1</op>
		</mult>
      </value>
      <valueInc>0.1</valueInc>
      <label>Standby L3 Current</label>
      <sensorSet>Generator Engine Sensors</sensorSet>
    </numSensor>
    <numSensor ruleid="gen_0_oilTemp">
      <type>temp</type>
      <sensorId>GEN0_OIL_TEMP</sensorId>
      <value>
		<mult>
		<op>
			<sub>
			<op>
				<mult>
				<op><getOid>GEN0_OIL_TEMP</getOid></op>
				<op>0.1</op>
				</mult>
			</op>
			<op>32</op>
			</sub>
		</op>
		<op><div><op>5</op><op>9</op></div></op>
		</mult>
      </value>
      <valueInc>0.1</valueInc>
      <label>Oil Temperature</label>
      <sensorSet>Generator Engine Sensors</sensorSet>
    </numSensor>
	<numSensor ruleid="gen_0_totalFuelConsumption">
      <type>num</type>
		<units>liters</units>
      <sensorId>GEN0_FUEL_CONSUMPTION</sensorId>
      <value>
		<mult>
		<op><getOid>GEN0_FUEL_CONSUMPTION</getOid></op>
		<op>0.378541</op>
		</mult>
      </value>
      <valueInc>0.1</valueInc>
      <label>Total Fuel Consumption</label>
      <sensorSet>Generator Engine Sensors</sensorSet>
    </numSensor>

		<alarmFlags ruleid="faultStatus1">
			<value><getOid>GEN0_FAULT_STATUS_1</getOid></value>
			<alarmFlagsTest mask="80" match="80">
				<name>
					<activeText>Engine Coolant Temp 1: Vtg Above Normal</activeText>
					<inactiveText>Engine Coolant Temp 1: Vtg Above Normal Cleared</inactiveText>
				</name>
				<severity>warning</severity>
				<category>System</category>
				<description>
					<activeText>An Engine Coolant Temp 1: Vtg Above Normal condition exists.</activeText>
					<inactiveText>Engine Coolant Temp 1: Vtg Above Normal condition no longer exists.</inactiveText>
				</description>
			</alarmFlagsTest>
			<alarmFlagsTest mask="100" match="100">
				<name>
					<activeText>Engine Coolant Temp 1: Vtg Below Normal</activeText>
					<inactiveText>Engine Coolant Temp 1: Vtg Below Normal Cleared</inactiveText>
				</name>
				<severity>warning</severity>
				<category>System</category>
				<description>
					<activeText>An Engine Coolant Temp 1: Vtg Below Normal condition exists.</activeText>
					<inactiveText>Engine Coolant Temp 1: Vtg Below Normal condition no longer exists.</inactiveText>
				</description>
			</alarmFlagsTest>
		</alarmFlags>
		<alarmFlags ruleid="faultStatus2">
			<value><getOid>GEN0_FAULT_STATUS_2</getOid></value>
			<alarmFlagsTest mask="1" match="1">
				<name>
					<activeText>Coolant Level Sensor 1: Vtg Below Normal</activeText>
					<inactiveText>Coolant Level Sensor 1: Vtg Below Normal Cleared</inactiveText>
				</name>
				<severity>warning</severity>
				<category>System</category>
				<description>
					<activeText>An Coolant Level Sensor 1: Vtg Below Normal condition exists.</activeText>
					<inactiveText>Coolant Level Sensor 1: Vtg Below Normal condition no longer exists.</inactiveText>
				</description>
			</alarmFlagsTest>
			<alarmFlagsTest mask="4" match="4">
				<name>
					<activeText>Engine Oil Temp Sensor 1: Vtg Above Normal</activeText>
					<inactiveText>Engine Oil Temp Sensor 1: Vtg Above Normal Cleared</inactiveText>
				</name>
				<severity>warning</severity>
				<category>System</category>
				<description>
					<activeText>An Engine Oil Temp Sensor 1: Vtg Above Normal condition exists.</activeText>
					<inactiveText>Engine Oil Temp Sensor 1: Vtg Above Normal condition no longer exists.</inactiveText>
				</description>
			</alarmFlagsTest>
			<alarmFlagsTest mask="8" match="8">
				<name>
					<activeText>Engine Oil Temp Sensor 1: Vtg Below Normal</activeText>
					<inactiveText>Engine Oil Temp Sensor 1: Vtg Below Normal Cleared</inactiveText>
				</name>
				<severity>warning</severity>
				<category>System</category>
				<description>
					<activeText>An Engine Oil Temp Sensor 1: Vtg Below Normal condition exists.</activeText>
					<inactiveText>Engine Oil Temp Sensor 1: Vtg Below Normal condition no longer exists.</inactiveText>
				</description>
			</alarmFlagsTest>
			<alarmFlagsTest mask="400" match="400">
				<name>
					<activeText>Coolant Pressure: Below Normal (Most Severe)</activeText>
					<inactiveText>Coolant Pressure: Below Normal (Most Severe) Cleared</inactiveText>
				</name>
				<severity>warning</severity>
				<category>System</category>
				<description>
					<activeText>A Coolant Pressure: Below Normal (Most Severe) condition exists.</activeText>
					<inactiveText>Coolant Pressure: Below Normal (Most Severe) condition no longer exists.</inactiveText>
				</description>
			</alarmFlagsTest>
			<alarmFlagsTest mask="800" match="800">
				<name>
					<activeText>Coolant Pressure Sensor: Vtg Above Normal</activeText>
					<inactiveText>Coolant Pressure Sensor: Vtg Above Normal Cleared</inactiveText>
				</name>
				<severity>warning</severity>
				<category>System</category>
				<description>
					<activeText>A Coolant Pressure Sensor: Vtg Above Normal condition exists.</activeText>
					<inactiveText>Coolant Pressure Sensor: Vtg Above Normal condition no longer exists.</inactiveText>
				</description>
			</alarmFlagsTest>
			<alarmFlagsTest mask="1000" match="1000">
				<name>
					<activeText>Coolant Pressure Sensor: Vtg Below Normal</activeText>
					<inactiveText>Coolant Pressure Sensor: Vtg Below Normal Cleared</inactiveText>
				</name>
				<severity>warning</severity>
